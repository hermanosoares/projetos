<?xml version="1.0" encoding="UTF-8"?>
<page xmlns="http://jboss.com/products/seam/pages"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://jboss.com/products/seam/pages http://jboss.com/products/seam/pages-2.2.xsd"
      action="#{ConversationHelper.BeginJoinConversationManual}">
                 
<navigation>

	<rule if-outcome="cadastroUsuarioInstituicao">
		<begin-conversation join="true"/>
		<redirect view-id="/admin/usuario/cadastroui.xhtml"/>	
	</rule>

	<rule if-outcome="proximo">
		<begin-conversation join="true"/>
		<redirect view-id="/admin/usuario/cadastro2.xhtml"/>	
	</rule>

	<rule if-outcome="AtualizadoComSucesso">
		<end-conversation/>		
		<redirect view-id="/main.xhtml"/>	
	</rule>

	<rule if-outcome="CriadoComSucesso">
		<end-conversation/>		
		<redirect view-id="/login/instituicoesensino.xhtml"/>
	</rule>
	
	<rule if-outcome="usuariolist">
		<end-conversation before-redirect="true"/>
		<redirect view-id="/admin/usuario/usuariolist.xhtml"/>	
	</rule>	

	<rule if-outcome="MesmaPagina">
		<begin-conversation join="true"/>
		<redirect view-id="/admin/usuario/usuario.xhtml"/>	
	</rule>	

	<rule if-outcome="cancelaAtualizacao">
		<end-conversation before-redirect="true"/>		
		<redirect view-id="/main.xhtml"/>
	</rule>	
	
</navigation>

	
</page>