package br.com.bulktecnologia.modelo.enums;

public enum TipoBloqueioAcesso {
	
	/**
	 * acesso do usuario esta relacionado a um periodo
	 * de acesso permitido.
	 */
	TERMINO_PERIODO_ACESSO,
	
	/**
	 * Existem debitos financeiros em aberto.
	 */
	PAGAMENTO,
	
	/**
	 * por algum motivo usuario nao tem mais interesse em
	 * utilizar o sistema. 
	 */
	USUARIO_DESCONTINUADO,

	BLOQUEIO_REALIZADO_POR_USUARIO_TECNICO_INSTITUICAO
	
}
