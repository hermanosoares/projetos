<?xml version="1.0" encoding="UTF-8"?>
<page xmlns="http://jboss.com/products/seam/pages"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://jboss.com/products/seam/pages http://jboss.com/products/seam/pages-2.2.xsd"
      no-conversation-view-id="/uc/matricula/matriculalist.xhtml"
      conversation-required="true">


<navigation>

	<rule if-outcome="cancela">
		<end-conversation before-redirect="true"/>
		<redirect view-id="/uc/matricula/alunocommatriculalist.xhtml"/>
	</rule>
		
	<rule if-outcome="list">
		<begin-conversation join="true"/>
		<redirect view-id="/uc/matricula/detalhematriculalist.xhtml"/>
	</rule>
		
		
	<rule if-outcome="add_edit">
		<begin-conversation join="true"/>
		<redirect view-id="/uc/matricula/alunocommatriculalist.xhtml"/>
	</rule>

</navigation>

</page>