public enum TipoSerie implements Serializable{

	Primeiro("1"),
	Segundo("2"),
	Terceiro("3"),
	Quarto("4"),
	Quinto("5"),
	Sexto("6"),
	Setimo("7"),
	Oitavo("8"),
	Nono("9");
	
	private String label;
	
	private TipoSerie(String label){
		this.label = label;
	}
	
    
	@Override
	public String toString() {
		return this.label;
	}


	public String getLabel() {
		return label;
	}
	
	public String getSigla(){
		return this.label;
	}
	
}
