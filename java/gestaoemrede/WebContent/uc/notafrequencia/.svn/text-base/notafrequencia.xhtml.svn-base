<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml"
      xmlns:h="http://java.sun.com/jsf/html"
      xmlns:f="http://java.sun.com/jsf/core"
      xmlns:ui="http://java.sun.com/jsf/facelets"
      xmlns:a4j="http://richfaces.org/a4j"
      xmlns:rich="http://richfaces.org/rich"
      xmlns:s="http://jboss.com/products/seam/taglib">

	<ui:composition template="/template.xhtml">
	
	<ui:define name="navegacao">
		<div id="topoBotao">
			<h:commandButton styleClass="botaoNavegacao" image="/img/gravar.jpg" value="gravar" action="#{#}"/>
			<h:commandButton styleClass="botaoNavegacaoEspaco" immediate="true" action="#{#}" value="cancelar" image="/img/cancelar.jpg" />
		</div>
	</ui:define>
	
		<ui:define name="conteudo">
			
			<rich:dataTable styleClass="tabelaLancamentoNota" value="#" rowClasses="ClasseCss2,ClasseCss1" var="#" rendered="true">
				
				<h:column headerClass="tabelaLancamentoNotaColumnNome">
					<f:facet name="header">
						<h:outputText value="Nome" />			
					</f:facet>
					
					<rich:inplaceInput value="#{#}"/>
				</h:column>
				
				
				<h:column headerClass="tabelaLancamentoNotaColumnNota">
					<f:facet name="header">
						<h:outputText value="Nota" />			
					</f:facet>
					
					<h:inputText styleClass="inputNotaFrequencia" value="#" />
				</h:column>
				
				<h:column headerClass="tabelaLancamentoNotaColumnFrequencia">
					<f:facet name="header">
						<h:outputText value="Frequência" />			
					</f:facet>
					<h:inputText styleClass="inputNotaFrequencia" value="#" />
					
				</h:column>
				
				
				
				
				
			
			</rich:dataTable>
		
		</ui:define>

	</ui:composition>

</html>